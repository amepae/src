if not Ignore then
    Ignore = {} -- Add Instances to this table to ignore them (e.g. Ignore = {workspace.Map, workspace.Map2})
end
if SendNotifications == nil then
    SendNotifications = true -- Set to false if you don't want notifications
end
if ConsoleLogs == nil then
    ConsoleLogs = false -- Set to true if you want console logs (mainly for debugging)
end

if not game:IsLoaded() then
    repeat
        task.wait()
    until game:IsLoaded()
end
if not Settings then
    Settings = {
        Players = {
            ["Ignore Me"] = true,
            ["Ignore Others"] = true,
            ["Ignore Tools"] = true
        },
        Meshes = {
            NoMesh = false,
            NoTexture = false,
            Destroy = false
        },
        Images = {
            Invisible = true,
            Destroy = false
        },
        Explosions = {
            Smaller = true,
            Invisible = false, -- Not recommended for PVP games
            Destroy = false -- Not recommended for PVP games
        },
        Particles = {
            Invisible = true,
            Destroy = false
        },
        TextLabels = {
            LowerQuality = true, -- false
            Invisible = false,
            Destroy = false
        },
        MeshParts = {
            LowerQuality = true,
            Invisible = false,
            NoTexture = false,
            NoMesh = false,
            Destroy = false
        },
        Other = {
            ["FPS Cap"] = true, -- Set this true to uncap FPS
            ["No Camera Effects"] = true,
            ["No Clothes"] = true,
            ["Low Water Graphics"] = true,
            ["No Shadows"] = true,
            ["Low Rendering"] = true,
            ["Low Quality Parts"] = true,
            ["Low Quality Models"] = true,
            ["Reset Materials"] = true,
            ["Lower Quality MeshParts"] = true,
            ClearNilInstances = false
        }
    }
end
local Players, Lighting, StarterGui, MaterialService = game:GetService("Players"), game:GetService("Lighting"), game:GetService("StarterGui"), game:GetService("MaterialService")
local ME, CanBeEnabled = Players.LocalPlayer, {"ParticleEmitter", "Trail", "Smoke", "Fire", "Sparkles"}
local function Notify(msg)
    StarterGui:SetCore("SendNotification",{
        Title = "FPS Booster",
        Text = msg,
        Duration = 5
    })
end
local function PartOfCharacter(Inst)
    for i, v in pairs(Players:GetPlayers()) do
        if v ~= ME and v.Character and Inst:IsDescendantOf(v.Character) then
            return true
        end
    end
    return false
end
local function DescendantOfIgnore(Inst)
    for i, v in pairs(Ignore) do
        if Inst:IsDescendantOf(v) then
            return true
        end
    end
    return false
end
local function CheckIfBad(Inst)
    if not Inst:IsDescendantOf(Players) and (Settings.Players["Ignore Others"] and not PartOfCharacter(Inst) 
    or not Settings.Players["Ignore Others"]) and (Settings.Players["Ignore Me"] and ME.Character and not Inst:IsDescendantOf(ME.Character) 
    or not Settings.Players["Ignore Me"]) and (Settings.Players["Ignore Tools"] and not Inst:IsA("BackpackItem") and not Inst:FindFirstAncestorWhichIsA("BackpackItem") 
    or not Settings.Players["Ignore Tools"]) and (Ignore and not table.find(Ignore, Inst) and not DescendantOfIgnore(Inst) 
    or (not Ignore or type(Ignore) ~= "table" or #Ignore <= 0)) then
        if Inst:IsA("DataModelMesh") then
            if Inst:IsA("SpecialMesh") then
                if Settings.Meshes.NoMesh then
                    Inst.MeshId = ""
                end
                if Settings.Meshes.NoTexture then
                    Inst.TextureId = ""
                end
            end
            if Settings.Meshes.Destroy or Settings["No Meshes"] then
                Inst:Destroy()
            end
        elseif Inst:IsA("FaceInstance") then
            if Settings.Images.Invisible then
                Inst.Transparency = 1
                Inst.Shiny = 1
            end
            if Settings.Images.LowDetail then
                Inst.Shiny = 1
            end
            if Settings.Images.Destroy then
                Inst:Destroy()
            end
        elseif Inst:IsA("ShirtGraphic") then
            if Settings.Images.Invisible then
                Inst.Graphic = ""
            end
            if Settings.Images.Destroy then
                Inst:Destroy()
            end
        elseif table.find(CanBeEnabled, Inst.ClassName) then
            if Settings["Invisible Particles"] or Settings["No Particles"] or (Settings.Other and Settings.Other["Invisible Particles"]) or (Settings.Particles and Settings.Particles.Invisible) then
                Inst.Enabled = false
            end
            if (Settings.Other and Settings.Other["No Particles"]) or (Settings.Particles and Settings.Particles.Destroy) then
                Inst:Destroy()
            end
        elseif Inst:IsA("PostEffect") and (Settings["No Camera Effects"] or (Settings.Other and Settings.Other["No Camera Effects"])) then
            Inst.Enabled = false
        elseif Inst:IsA("Explosion") then
            if Settings["Smaller Explosions"] or (Settings.Other and Settings.Other["Smaller Explosions"]) or (Settings.Explosions and Settings.Explosions.Smaller) then
                Inst.BlastPressure = 1
                Inst.BlastRadius = 1
            end
            if Settings["Invisible Explosions"] or (Settings.Other and Settings.Other["Invisible Explosions"]) or (Settings.Explosions and Settings.Explosions.Invisible) then
                Inst.BlastPressure = 1
                Inst.BlastRadius = 1
                Inst.Visible = false
            end
            if Settings["No Explosions"] or (Settings.Other and Settings.Other["No Explosions"]) or (Settings.Explosions and Settings.Explosions.Destroy) then
                Inst:Destroy()
            end
        elseif Inst:IsA("Clothing") or Inst:IsA("SurfaceAppearance") or Inst:IsA("BaseWrap") then
            if Settings["No Clothes"] or (Settings.Other and Settings.Other["No Clothes"]) then
                Inst:Destroy()
            end
        elseif Inst:IsA("BasePart") and not Inst:IsA("MeshPart") then
            if Settings["Low Quality Parts"] or (Settings.Other and Settings.Other["Low Quality Parts"]) then
                Inst.Material = Enum.Material.Plastic
                Inst.Reflectance = 0
            end
        elseif Inst:IsA("TextLabel") and Inst:IsDescendantOf(workspace) then
            if Settings["Lower Quality TextLabels"] or (Settings.Other and Settings.Other["Lower Quality TextLabels"]) or (Settings.TextLabels and Settings.TextLabels.LowerQuality) then
                Inst.Font = Enum.Font.SourceSans
                Inst.TextScaled = false
                Inst.RichText = false
                Inst.TextSize = 14
            end
            if Settings["Invisible TextLabels"] or (Settings.Other and Settings.Other["Invisible TextLabels"]) or (Settings.TextLabels and Settings.TextLabels.Invisible) then
                Inst.Visible = false
            end
            if Settings["No TextLabels"] or (Settings.Other and Settings.Other["No TextLabels"]) or (Settings.TextLabels and Settings.TextLabels.Destroy) then
                Inst:Destroy()
            end
        elseif Inst:IsA("Model") then
            if Settings["Low Quality Models"] or (Settings.Other and Settings.Other["Low Quality Models"]) then
                Inst.LevelOfDetail = 1
            end
        elseif Inst:IsA("MeshPart") then
            if Settings["Low Quality MeshParts"] or (Settings.Other and Settings.Other["Low Quality MeshParts"]) or (Settings.MeshParts and Settings.MeshParts.LowerQuality) then
                Inst.RenderFidelity = 2
                Inst.Reflectance = 0
                Inst.Material = Enum.Material.Plastic
            end
            if Settings["Invisible MeshParts"] or (Settings.Other and Settings.Other["Invisible MeshParts"]) or (Settings.MeshParts and Settings.MeshParts.Invisible) then
                Inst.Transparency = 1
                Inst.RenderFidelity = 2
                Inst.Reflectance = 0
                Inst.Material = Enum.Material.Plastic
            end
            if Settings.MeshParts and Settings.MeshParts.NoTexture then
                Inst.TextureID = ""
            end
            if Settings.MeshParts and Settings.MeshParts.NoMesh then
                Inst.MeshId = ""
            end
            if Settings["No MeshParts"] or (Settings.Other and Settings.Other["No MeshParts"]) or (Settings.MeshParts and Settings.MeshParts.Destroy) then
                Inst:Destroy()
            end
        end
    end
end
if SendNotifications then
	Notify("Loading FPS Booster...")
end
coroutine.wrap(pcall)(function()
    if (Settings["Low Water Graphics"] or (Settings.Other and Settings.Other["Low Water Graphics"])) then
        local terrain = workspace:FindFirstChildOfClass("Terrain")
        if not terrain then
            repeat
                task.wait()
            until workspace:FindFirstChildOfClass("Terrain")
            terrain = workspace:FindFirstChildOfClass("Terrain")
        end
        terrain.WaterWaveSize = 0
        terrain.WaterWaveSpeed = 0
        terrain.WaterReflectance = 0
        terrain.WaterTransparency = 0
        if sethiddenproperty then
            sethiddenproperty(terrain, "Decoration", false)
        else
	Notify("Your exploit does not support sethiddenproperty, please use a different exploit.")
        end
        if ConsoleLogs then
            warn("Low Water Graphics Enabled")
        end
    end
end)
coroutine.wrap(pcall)(function()
    if Settings["No Shadows"] or (Settings.Other and Settings.Other["No Shadows"]) then
        Lighting.GlobalShadows = false
        Lighting.FogEnd = 9e9
        Lighting.ShadowSoftness = 0
        if sethiddenproperty then
            sethiddenproperty(Lighting, "Technology", 2)
        else
	    Notify("Your exploit does not support sethiddenproperty, please use a different exploit.")
        end
        if ConsoleLogs then
            warn("No Shadows Enabled")
        end
    end
end)
coroutine.wrap(pcall)(function()
    if Settings["Low Rendering"] or (Settings.Other and Settings.Other["Low Rendering"]) then
        settings().Rendering.QualityLevel = 1
        settings().Rendering.MeshPartDetailLevel = Enum.MeshPartDetailLevel.Level04
        if ConsoleLogs then
            warn("Low Rendering Enabled")
        end
    end
end)
coroutine.wrap(pcall)(function()
    if Settings["Reset Materials"] or (Settings.Other and Settings.Other["Reset Materials"]) then
        for i, v in pairs(MaterialService:GetChildren()) do
            v:Destroy()
        end
        MaterialService.Use2022Materials = false
        if ConsoleLogs then
            warn("Reset Materials Enabled")
        end
    end
end)
coroutine.wrap(pcall)(function()
    if Settings["FPS Cap"] or (Settings.Other and Settings.Other["FPS Cap"]) then
        if setfpscap then
            if type(Settings["FPS Cap"] or (Settings.Other and Settings.Other["FPS Cap"])) == "string" or type(Settings["FPS Cap"] or (Settings.Other and Settings.Other["FPS Cap"])) == "number" then
                setfpscap(tonumber(Settings["FPS Cap"] or (Settings.Other and Settings.Other["FPS Cap"])))
                if ConsoleLogs then
                    warn("FPS Capped to " .. tostring(Settings["FPS Cap"] or (Settings.Other and Settings.Other["FPS Cap"])))
                end
            elseif Settings["FPS Cap"] or (Settings.Other and Settings.Other["FPS Cap"]) == true then
                setfpscap(1e6)
                if ConsoleLogs then
                    warn("FPS Uncapped")
                end
            end
        else
	Notify("FPS Cap Failed")
        end
    end
end)
coroutine.wrap(pcall)(function()
    if Settings.Other["ClearNilInstances"] then
        if getnilinstances then
            for _, v in pairs(getnilinstances()) do
                pcall(v.Destroy, v)
            end
            if ConsoleLogs then
		warn("Cleared Nil Instances")
            end
        else
	Notify("Your exploit does not support getnilinstances, please use a different exploit.")
        end
    end
end)
local Descendants = game:GetDescendants()
if ConsoleLogs then
    warn("Checking " .. #Descendants .. " Instances...")
end
for i, v in pairs(Descendants) do
    CheckIfBad(v)
end

game.DescendantAdded:Connect(function(value)
    wait(LoadedWait or 1)
    CheckIfBad(value)
end)
